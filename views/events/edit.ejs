<%- include('../partials/header.ejs')%>

    <h1>Event Edit Page</h1>
        <form action="/events/<%=events._id%>?_method=PUT" method="POST">
        <input type="text" name="name" value="<%=events.name%>"/> <br/>
        <input type="text" name="artists" value="<%=events.artists%>"/> <br/>
        <input type="text" name="date" value="<%=events.date%>"/> <br/>
        <input type="text" name="playlist" value="<%=events.playlist%>"/> <br/>
        <input type="text" name="venue" value="<%=events.venue%>"/> <br/>
        <input type="text" name="picture" value="<%=events.picture%>"/> <br/>
        <button type="submit">Update</button>
    </form>
<%- include('../partials/footer.ejs')%>